{
    "activationEvents": [
        "onCommand:i18n-ext.addKeyAndTranslation",
        "onCommand:i18n-ext.addKeyToInterface",
        "onCommand:i18n-ext.addTranslationToCultureFiles"
    ],
    "author": {
        "name": "Brandon Scott",
        "url": "https://github.com/brandongregoryscott"
    },
    "categories": [
        "Other"
    ],
    "contributes": {
        "commands": [
            {
                "command": "i18n-ext.addKeyAndTranslation",
                "title": "i18n-ext: Add key to interface and translation to culture files"
            },
            {
                "command": "i18n-ext.addKeyToInterface",
                "title": "i18n-ext: Add key to interface"
            },
            {
                "command": "i18n-ext.addTranslationToCultureFiles",
                "title": "i18n-ext: Add translation to culture files"
            }
        ],
        "configuration": {
            "properties": {
                "i18n-ext.cultureFilePaths": {
                    "default": [
                        "**/cultures/*.ts"
                    ],
                    "description": "Array of paths/glob patterns to culture files",
                    "type": "array"
                },
                "i18n-ext.cultureInterfacePath": {
                    "default": "**/interfaces/culture-resources.ts",
                    "description": "Path/glob pattern to interface defining typed i18n keys",
                    "type": "string"
                },
                "i18n-ext.insertionPosition": {
                    "default": [
                        "Loose Alphabetical"
                    ],
                    "description": "Position to insert key/translation into file",
                    "enum": [
                        "End",
                        "Loose Alphabetical",
                        "Start",
                        "Strict Alphabetical"
                    ],
                    "enumDescriptions": [
                        "Inserts key at the end of the file/object",
                        "Inserts key in best guess alphabetical order (Assumes file is already sorted)",
                        "Inserts key at the start of the file/object",
                        "Inserts key in alphabetical order (Performs a full sort on file - slow)"
                    ],
                    "type": "string"
                }
            },
            "title": "i18n-ext"
        }
    },
    "dependencies": {
        "google-translate-api": "^2.3.0",
        "ts-morph": "10.0.1"
    },
    "description": "Ease the burden of adding localized keys and strings to multiple typed culture files",
    "devDependencies": {
        "@types/glob": "7.1.3",
        "@types/mocha": "8.0.4",
        "@types/node": "12.11.7",
        "@types/vscode": "1.54.0",
        "@typescript-eslint/eslint-plugin": "4.14.1",
        "@typescript-eslint/parser": "4.14.1",
        "eslint": "7.19.0",
        "glob": "7.1.6",
        "mocha": "8.2.1",
        "prettier": "2.2.1",
        "rimraf": "3.0.2",
        "typescript": "4.1.3",
        "vscode-test": "1.5.0"
    },
    "displayName": "i18n-ext",
    "engines": {
        "vscode": "^1.54.0"
    },
    "keywords": [
        "internationalization",
        "localization",
        "translations",
        "typescript"
    ],
    "license": "Apache-2.0",
    "main": "./dist/extension.js",
    "name": "i18n-ext",
    "repository": {
        "type": "git",
        "url": "git@github.com:brandongregoryscott/i18n-ext.git"
    },
    "scripts": {
        "build": "tsc --pretty --project tsconfig.json",
        "clean": "rimraff dist",
        "lint": "eslint src --ext ts",
        "postpublish": "git add -A && git commit -m \"$npm_package_version\" && git push origin main",
        "prebuild": "npm run clean",
        "prepublishOnly": "npm run build",
        "pretest": "npm run build && npm run lint",
        "test": "node ./dist/test/runTest.js",
        "vscode:prepublish": "npm run build",
        "watch": "tsc --pretty --watch"
    },
    "version": "0.0.1"
}
